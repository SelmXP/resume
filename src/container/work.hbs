<div>
    {{#with main}}
        <main>
            <section>
                {{> title data='Work history' big=true}}
                <div>
                    <ul>
                        {{#each work}}
                            <li>
                                <div>{{position}}</div>
                                <br>
                                <div>{{duration.from}} - {{duration.to}}</div>
                                <br>
                                <div>{{> link text=company.name href=company.url blank=true noLine=true }}</div>
                            </li>
                            <br>
                        {{/each}}
                    </ul>
                </div>

            </section>

           
        </main>
    {{/with}}
</div>