<div>
    {{#with main}}
    <main>
        <section class="ps-0">
            {{> title data='My education' big=true }}
            <ul>
                {{#each education}}
                <li>
                    {{name}}
                    {{#unless isEnd}} (ще в процесі) {{/unless}}
                </li>
                {{/each}}

            </ul>
        </section>

        <section class="ps-0">
            {{> title data='My certificates' big=false}}
            <ul>
                {{#each certificates}}
                <li>
                    {{name}} ({{id}})
                </li>
                {{/each}}
            </ul>
        </section>
    </main>
    {{/with}}
</div>